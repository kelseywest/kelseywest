    var webInitData = {};
    var xhr = new XMLHttpRequest();
    xhr.open("POST", '/ajax_init.php', false);
    xhr.onload = function() {
        if(xhr.status == 200) {
            var response = xhr.response;
            var result = JSON.parse(response);
            if (result.webInitData) {
                webInitData = result.webInitData;
            }
        }
    };
    xhr.send();
</script>

