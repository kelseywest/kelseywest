`
            tempNode.innerHTML = html;
            return tempNode.firstElementChild;
        }
        this.bind = function(el, fun) {
            const _this = this

        }
        this.append=function(){
            const _this = this
            var domHtml = _this.generateHtml()
            _this.bind(domHtml)
            appendDom.appendChild(domHtml)
        }
    }
    var titleObj = new divideBanner(initConfig)
    titleObj.append()
    
    
        
    </script>
        

        


        
